<script>
	$( document ).ready(function() {
		jQuery('.tab_section_landing .but').each(function () {
			if(jQuery(this).hasClass('active') == true) {
				x = jQuery(this).index();
				jQuery('.tab_section_landing .but').eq(x).click();
			}
		})
	});
	jQuery('.cl-div').eq(0).addClass('active-tab');
	jQuery('.tab_section_landing .but').click(function(){
	    jQuery(this).addClass('active').siblings().removeClass('active');
	    let this_index = jQuery(this).index();
	    jQuery('.cl-div').eq(this_index).addClass('active-tab').siblings().removeClass('active-tab');
	})


	/*most popular lists*/
	if(jQuery(window).width() > 767) {
		jQuery('.most-populars .wrap-btn').click(function() {
			jQuery(this).toggleClass('active');
			if (jQuery(this).hasClass('active') == true) {
				jQuery(this).parent().find('.wraprer').slideDown("slow");
				jQuery(this).find('button').text('-');
				jQuery(this).find('p').text('Hide Full List');

			} else {
				jQuery('.most-populars .wrap-container .wraprer:not(:first-child)').slideUp("slow");
				jQuery(this).find('button').text('+');
				jQuery(this).find('p').text('View Full List');
			}
		});
		

	} else {
		// modal tabs
		let r_list = jQuery('.most-populars .flex-div ul + ul').html();
		jQuery('.most-populars .flex-div ul').first().append(r_list);
		jQuery('.most-populars .title').click(function() {
			jQuery(this).toggleClass('opened').next('.flex-div').slideToggle();
		})
	}

	jQuery('.mob_drow_btn').click(function() {
		jQuery(this).addClass('actived');
		jQuery('.tab_section_landing .but').fadeIn();
		jQuery('.tab_section_landing .but').click(function() {
			jQuery('.mob_drow_btn').removeClass('actived');
			jQuery('.tab_section_landing .but').not(this).fadeOut();
		})
	})

	jQuery('.tab_section_landing .open_modal_custom').click((e) => {
		e.preventDefault();
    	jQuery('.cleaning_modal').show(0).addClass('opened');
    })
    jQuery('body').on('click', '.cleaning_modal_close', function() {
    	jQuery('.cleaning_modal').removeClass('opened');
    	setTimeout(() => {
    		jQuery('.cleaning_modal').hide(0);
    		jQuery('.cleaning_modal .some-tab button').removeClass('active');
    	}, 1100);

    	getIndexContent = jQuery('.cleaning_modal .some-tab .active').index();
    	jQuery('.some-tab button').eq(getIndexContent).click();

    });
    // Modal JS
    jQuery('.see_pricing').click(function() {
    	jQuery('.cleaning_modal .cl-div').removeClass('active-tab');
    	jQuery('.cleaning_modal .cl-div').eq(0).addClass('active-tab');
    	jQuery('.cleaning_modal .some-tab button').first().addClass('active');

    	getIndexModal = jQuery('.some-tab .active').index();
    	jQuery('.cleaning_modal_cont button').eq(getIndexModal).click();
    });
    jQuery('.cleaning_modal .some-tab button').click(function(){
	    jQuery(this).addClass('active').siblings().removeClass('active');
	    jQuery('.modal_btn').css('display', 'block');
	    let this_index = jQuery(this).index();
	    jQuery('.cleaning_modal .cl-div').eq(this_index).addClass('active-tab').siblings().removeClass('active-tab');
	})



	/* modal text */

	jQuery('.tab_section_landing .open_modal_custom_text').click((e) => {
		e.preventDefault();
    	jQuery('.cleaning_modal_text').show(0).addClass('opened');
    })
	jQuery('.open_modal_custom_text').click(function(){
    	htmlModal = jQuery(this).parent().parent().parent().find('.wrap_modal').html();
    	jQuery('.cleaning_modal_text .cleaning_modal_cont_text').html(htmlModal);
    })
    jQuery('body').on('click', '.cleaning_modal_close_text', function() {
    	jQuery('.cleaning_modal_text').removeClass('opened');
    	setTimeout(() => {
    		jQuery('.cleaning_modal_text').hide(0);
    	}, 1100);


    }); 
</script>


